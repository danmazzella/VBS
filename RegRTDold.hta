	Sub PcOffline_onclick()
		Dim SH,fso
		Set FSO = CreateObject("Scripting.FileSystemObject")
		Set SH = CreateObject("WScript.Shell") 
		if fso.fileexists("c:\temp\danscripts\RegRTD\PcOffline.txt") then

<HTML>
<HTA:APPLICATION APPLICATIONNAME="Not as Commonly Used"
    CONTEXTMENU=no SCROLL=no MAXIMIZEBUTTON=no SINGLEINSTANCE="yes" WindowsState = "normal" ICON="NotUsed.ico"/> 
<HEAD>
<TITLE>Not As Commonly Used</TITLE>

<style>
.clear { /* generic container (i.e. div) for floating buttons */
    overflow: hidden;
    width: 100%;
}

a.button {
    background: transparent url('bg_button.png') no-repeat scroll top right;
    color: #444;
    display: block;
    float: left;
    font: normal 12px arial, sans-serif;
    height: 24px;
    margin-right: 6px;
    padding-right: 18px; /* sliding doors padding */
    text-decoration: none;
}

a.button span {
    background: transparent url('bg_button_span.png') no-repeat;
    display: block;
    line-height: 14px;
    padding: 5px 0 5px 18px;
}

a.button:active {
    background-position: bottom right;
    color: #000;
    outline: none; /* hide dotted outline in Firefox */
}

a.button:active span {
    background-position: bottom left;
    padding: 6px 0 4px 18px; /* push text down 1px */
}
</style>

<SCRIPT language=vbscript>

	Sub MainScript_onclick()
		Set WshShell = CreateObject("WScript.Shell")
		WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\DanScripts.exe""", 1
		window.close()
	End sub

	Sub cmdContinue_onclick()
		Set WshShell = CreateObject("WScript.Shell")
		Set fso = CreateObject("Scripting.FileSystemObject")
		choice = document.NotUsed.choice.selectedIndex
		
		Set fso = CreateObject("Scripting.FileSystemObject")
		Set objNTInfo = CreateObject("WinNTSystemInfo")
		Set WriteStuff = FSO.OpenTextFile("\\jc1wsalt03\express\temp\DanLog\Log.csv", 8, True)
		WriteStuff.Write(Date & ", " & Time & ", ")
		WriteStuff.Write(objNTInfo.ComputerName & ", " & objNTInfo.UserName & ", ") 
		
		if choice = 0 then
			WriteStuff.Write("Show connected Monitors" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\monitor.vbs""", 1
		elseif choice = 1 then
			WriteStuff.Write("Print Connected Monitors" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\monitor2.vbs""", 1
		elseif choice = 2 then
			WriteStuff.Write("Add list of ppl to RDP" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\addlisttordp.vbs""", 1
		elseif choice = 3 then
			WriteStuff.Write("Check bit per Pixel" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\bitperpixel.hta""", 1
		elseif choice = 4 then
			WriteStuff.Write("Check registry for VCRTD" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\checkregistry.hta""", 1
		elseif choice = 5 then
			WriteStuff.Write("u:\copy.bat" & VbCrLf)
			WshShell.Run """\\jc1dfs2\applications\copy.bat""", 1
		elseif choice = 6 then
			WriteStuff.Write("Copy file from I Drive" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\copyfileaim.vbs""", 1
		elseif choice = 7 then
			WriteStuff.Write("Check Video Card Settings" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\displaysettings.vbs""", 1
		elseif choice = 8 then
			WriteStuff.Write("Print Video Card Settings" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\displaysettingsprintfile.vbs""", 1
		elseif choice = 9 then
			WriteStuff.Write("Read or Set Page File" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\readsetpagefile.vbs""", 1
		elseif choice = 10 then
			WriteStuff.Write("Extract members of AD group" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\ExtractGroupMembers.vbs""", 1
		elseif choice = 11 then
			WriteStuff.Write("Search AD" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\searchvbs.vbs""", 1
		elseif choice = 12 then
			WriteStuff.Write("Export all of AD" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\export active directory.vbs""", 1
		elseif choice = 13 then
			WriteStuff.Write("Install RTD.CONFIG" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\RTDConfig.hta""", 1
		elseif choice = 14 then
			WriteStuff.Write("Check Java Version" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\javaversion.hta""", 1
		elseif choice = 15 then
			WriteStuff.Write("Copy Missing Java Files" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\Library\Packages\Dantools\copyjavafiles.vbs""", 1
		elseif choice = 16 then
			WriteStuff.Write("KTOP" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\KTOP.hta""", 1
		elseif choice = 17 then
			WriteStuff.Write("Laptop Loaner" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\LaptopLoaner.hta""", 1
		elseif choice = 18 then
			WriteStuff.Write("Restart Service on PC" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\RestartService.hta""", 1
		elseif choice = 19 then
			WriteStuff.Write("Remote Items from Startup" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\Packages\Dantools\RemoveStart.hta""", 1	
		elseif choice = 20 then
			WriteStuff.Write("Fix Remedy User Error" & VbCrLf)
			WshShell.Run "cscript.exe ""\\jc1wsalt03\library\packages\dantools\fixremedy.vbs""", 1
		elseif choice = 21 then
			WriteStuff.Write("Create Folders in the J:\15C2-11 Folder Folder" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\packages\dantools\15C2CreateFolders.hta""", 1
		elseif choice = 22 then
			WriteStuff.Write("Convert User/Full Name to Full/User Name" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\packages\dantools\ConvertUserFull.hta""", 1
		elseif choice = 23 then
			WriteStuff.Write("Clean DFS Folders" & VbCrLf)
			WshShell.Run """\\jc1wsalt03\Library\packages\dantools\CleanDFS.hta""", 1
		else
			WriteStuff.Write("Nothing Selected" & VbCrLf)
		end if
	End Sub

	Sub window_onload()
		window.resizeTo 600, 500
	End Sub
</SCRIPT>
</HEAD>
